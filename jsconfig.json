{
    "compilerOptions": {
        "baseUrl": "src",
        "jsx": "react",
        "paths": {
            "Style/*": [
                "src/style/*"
            ],
            "Component/*": [
                "src/component/*"
            ],
            "Route/*": [
                "src/route/*"
            ],
            "Store/*": [
                "src/store/*"
            ],
            "Util/*": [
                "src/util/*"
            ],
            "Api/*": [
                "src/query/*"
            ],
            "Type/*": [
                "src/type/*"
            ],
            "Pages/*": [
                "src/pages/*"
            ]
        }
    }
}
